Algoritmo "Media"

Var
   notaA : inteiro
   notaB : inteiro
   nome  : caractere
   media : real
Inicio
   escreva("Qual o nome o aluno?")
   leia(nome)
   escreva("Qual a primeira nota?")
   leia (notaA)
   escreva("Qual a segundo nota?")
   leia (notaB)
   media <- (notaA + notaB) /2
   escreva( " A média do aluno ",  nome ," foi ", media)



Fimalgoritmo