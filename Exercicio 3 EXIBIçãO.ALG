Algoritmo "Exibição"


Var
nome, cursoAtual : caractere
idade :inteiro

Inicio

  Escreval ("Qual o seu nome? ")
   leia(nome)
  Escreval ("Qual a sua idade? ")
   leia (idade)
  Escreval ("Qual o seu curso atual? ")
   leia (cursoAtual)
  Escreval ("Seu nome é = ", nome )
  Escreval ("Sua idade é = " , idade )
  Escreval ("Seu curso é = " ,cursoAtual )


Fimalgoritmo