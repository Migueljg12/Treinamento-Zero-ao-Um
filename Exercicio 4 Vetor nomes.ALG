Algoritmo "vetor nome"


var
pessoas:vetor[1..3] de caractere
numPessoa : inteiro

inicio

       escreval (" nome :")
        leia (pessoas[1])
       escreval (" nome :")
        leia (pessoas[2])
       escreval (" nome :")
        leia (pessoas[3])

       escreval ("Os nomes escritos foram: ", pessoas[1] ," ", pessoas[2] ," ", pessoas[3])
       
       
fimalgoritmo